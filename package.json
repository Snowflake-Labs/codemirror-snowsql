{
  "name": "codemirror-snowsql",
  "version": "0.14.1",
  "description": "a CodeMirror mode for the SnowSQL language",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "start": "webpack-dev-server --config webpack.config.cjs --open",
    "build": "bash ./build.sh",
    "build-app": "webpack --config webpack.config.cjs",
    "test": "node --experimental-specifier-resolution=node --experimental-modules --loader ts-node/esm node_modules/mocha/lib/cli/cli.js src/**/*.test.ts",
    "lint": "eslint src/ --ext .ts",
    "lint:fix": "eslint --fix src/ --ext .ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/prometheus-community/codemirror-snowSQL.git"
  },
  "keywords": [
    "snowSQL",
    "codemirror",
    "mode",
    "prometheus"
  ],
  "author": "Prometheus Authors <prometheus-developers@googlegroups.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/prometheus-community/codemirror-snowSQL/issues"
  },
  "homepage": "https://github.com/prometheus-community/codemirror-snowSQL/blob/master/README.md",
  "dependencies": {
    "lezer-snowsql": "^0.18.0",
    "lru-cache": "^6.0.0"
  },
  "devDependencies": {
    "@codemirror/autocomplete": "^0.18.3",
    "@codemirror/basic-setup": "^0.18.0",
    "@codemirror/highlight": "^0.18.3",
    "@codemirror/language": "^0.18.0",
    "@codemirror/lint": "^0.18.1",
    "@codemirror/state": "^0.18.2",
    "@codemirror/view": "^0.18.1",
    "@types/chai": "^4.2.12",
    "@types/lru-cache": "^5.1.0",
    "@types/mocha": "^8.0.3",
    "@types/node": "^14.0.13",
    "@typescript-eslint/eslint-plugin": "^2.22.0",
    "@typescript-eslint/parser": "^2.22.0",
    "chai": "^4.2.0",
    "clean-webpack-plugin": "^3.0.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-flowtype": "^5.2.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-prettier": "^3.1.4",
    "html-webpack-plugin": "^4.3.0",
    "mocha": "^8.1.2",
    "prettier": "^2.0.5",
    "ts-loader": "^7.0.4",
    "ts-node": "^9.0.0",
    "typescript": "^4.2.3",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.11.0"
  },
  "peerDependencies": {
    "@codemirror/autocomplete": "^0.18.3",
    "@codemirror/highlight": "^0.18.3",
    "@codemirror/language": "^0.18.0",
    "@codemirror/lint": "^0.18.1",
    "@codemirror/state": "^0.18.2",
    "@codemirror/view": "^0.18.1"
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "es5",
    "printWidth": 150
  },
  "engines": {
    "node": ">=12.0.0"
  }
}
